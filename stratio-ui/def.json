  "descriptors": {
    "components": [
      {
        "id": "avro",
        "component": "source",
        "type": "avro",
        "name": "Avro Source",
        "description": "*************************",
        "settings": {
          "bind": {
            "type": "string",
            "name": "Bind address",
            "description": "hostname or IP address to listen on",
            "default": "127.0.0.1",
            "required": true
          },
          "port": {
            "type": "string",
            "name": "Port to bind to",
            "description": "Port # to bind to",
            "default": "4141",
            "required": true
          },
          "threads": {
            "type": "",
            "name": "",
            "description": "Maximum number of worker threads to spawn",
            "default": "",
            "required": false
          },
          "compression-type": {
            "type": "",
            "name": "",
            "description": "This can be “none” or “deflate”. The compression-type must match the compression-type of matching AvroSource",
            "default": "none",
            "required": false
          },
          "ssl": {
            "type": "",
            "name": "",
            "description": "Set this to true to enable SSL encryption. You must also specify a “keystore” and a “keystore-password”.",
            "default": "false",
            "required": false
          },
          "keystore": {
            "type": "",
            "name": "",
            "description": "This is the path to a Java keystore file. Required for SSL.",
            "default": "",
            "required": false
          },
          "keystore-password": {
            "type": "",
            "name": "",
            "description": "The password for the Java keystore. Required for SSL.",
            "default": "",
            "required": false
          },
          "keystore-type": {
            "type": "",
            "name": "",
            "description": "The type of the Java keystore. This can be “JKS” or “PKCS12”.",
            "default": "",
            "required": false
          },
          "exclude-protocols": {
            "type": "",
            "name": "",
            "description": "Space-separated list of SSL/TLS protocols to exclude. SSLv3 will always be excluded in addition to the protocols specified.",
            "default": "",
            "required": false
          },
          "ipFilter": {
            "type": "",
            "name": "",
            "description": "Set this to true to enable ipFiltering for netty",
            "default": "false",
            "required": false
          },
          "ipFilter.rules": {
            "type": "",
            "name": "",
            "description": "Define N netty ipFilter pattern rules with this config.",
            "default": "",
            "required": false
          }
        },
        "ports": {
        },
        "ui": {
        }
