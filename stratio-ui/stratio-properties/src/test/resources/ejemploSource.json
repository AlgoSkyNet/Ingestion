{
    "sources": {
        "components": [
            {
                "id": "ircSource1",
                "component": "ircSource",
                "type": "irc",
                "name": "IRC Source",
                "description": "Connect IRC channels and retrieve messages from them.",
                "settings": [
                    {
                        "hostname": [
                            {
                                "type": "string",
                                "name": "Host name",
                                "description": "Target URI",
                                "default": "",
                                "required": true,
				"value": "/home"
                            }
                        ],
                        "port": [
                            {
                                "type": "integer",
                                "name": "Port",
                                "description": "IRC port",
                                "default": 6667,
                                "required": true,
				"value": ""
                            }
                        ],
                        "nick": [
                            {
                                "type": "string",
                                "name": "Nick",
                                "description": "Nickname",
                                "default": "",
                                "required": true
                            }
                        ],
                        "channels": [
                            {
                                "type": "string",
                                "name": "Channels",
                                "description": "Comma separated channels without hash. Example: ubuntu, trivial.",
                                "default": "",
                                "required": true
                            }
                        ],
                        "user": [
                            {
                                "type": "string",
                                "name": "User",
                                "description": "The username. Is used to register the connection.",
                                "default": "",
                                "required": false
                            }
                        ],
                        "name": [
                            {
                                "type": "string",
                                "name": "Name",
                                "description": "The realname. Is used to register the connection.",
                                "default": "",
                                "required": false
                            }
                        ],
                        "password": [
                            {
                                "type": "string",
                                "name": "Capacity",
                                "description": "Password. Required if you are registered.",
                                "default": "",
                                "required": false
                            }
                        ],
                        "replyPing": [
                            {
                                "type": "boolean",
                                "name": "Reply ping",
                                "description": "Automatically sends pong when receives a ping.",
                                "default": "false",
                                "required": false
                            }
                        ]
                    }
                ],
                "channels": {}
            },
            {
                "id": "redisSource1",
                "component": "redisSource",
                "type": "redis",
                "name": "Redis Source",
                "description": "Redis Subscribe Source with connection Pool.",
                "settings": [
                    {
                        "host": [
                            {
                                "type": "string",
                                "name": "Host",
                                "description": "Redis host",
                                "default": "localhost",
                                "required": true
                            }
                        ],
                        "port": [
                            {
                                "type": "integer",
                                "name": "Port",
                                "description": "Redis port",
                                "default": 6379,
                                "required": true
                            }
                        ],
                        "subscribe": [
                            {
                                "type": "string",
                                "name": "Subscribe",
                                "description": "Channels to subscribe. Comma separated channels values.",
                                "default": "",
                                "required": true
                            }
                        ],
                        "psubscribe": [
                            {
                                "type": "string",
                                "name": "User",
                                "description": "Channels to subscribe with given pattern.",
                                "default": "",
                                "required": false
                            }
                        ],
                        "charset": [
                            {
                                "type": "string",
                                "name": "Charset",
                                "description": "Charset",
                                "default": "UTF-8",
                                "required": false
                            }
                        ],
                        "property": [
                            {
                                "type": "string",
                                "name": "Property",
                                "description": "Prefix for pool properties. Set whatever property you want to Jedis Pool.",
                                "default": "",
                                "required": false
                            }
                        ]
                    }
                ],
                "channels": {}
            }
        ]
    }
}
